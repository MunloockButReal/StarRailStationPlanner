{
	"manifest_version": 3,
	"name": "HSRailStation Addon",
	"version": "1.4",
	"content_scripts": [
		{
			"matches": ["*://starrailstation.com/*"],
			"js": ["content.js"],
			"run_at": "document_idle"
		}
	],
	"web_accessible_resources": [
		{
			"resources": ["styles.css"],
			"matches": ["*://starrailstation.com/*"]
		}
	]
}
